{
  "name": "very-markova-11ty-theme",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx @11ty/eleventy --input=input --output=output --serve --watch",
    "build": "rm -rf output && npx @11ty/eleventy --input=input --output=output",
    "clean": "rm -rf output",
    "start": "npm run clean && npm run dev"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "html-minifier-terser": "^7.2.0"
  },
  "dependencies": {
    "eleventy-plugin-reading-time": "^0.0.1",
    "markdown-it": "^14.1.0",
    "reading-time": "^1.5.0"
  }
}
